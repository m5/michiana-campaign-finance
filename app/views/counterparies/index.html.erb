<p id="notice"><%= notice %></p>

<h1>Listing Counterparies</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Address</th>
      <th>City</th>
      <th>State</th>
      <th>Zip</th>
      <th>Occupation</th>
      <th>Office</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @counterparies.each do |counterpary| %>
      <tr>
        <td><%= counterpary.name %></td>
        <td><%= counterpary.address %></td>
        <td><%= counterpary.city %></td>
        <td><%= counterpary.state %></td>
        <td><%= counterpary.zip %></td>
        <td><%= counterpary.occupation %></td>
        <td><%= counterpary.office %></td>
        <td><%= link_to 'Show', counterpary %></td>
        <td><%= link_to 'Edit', edit_counterpary_path(counterpary) %></td>
        <td><%= link_to 'Destroy', counterpary, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Counterpary', new_counterpary_path %>
